{
  "name": "trovu",
  "version": "1.0.0",
  "description": "Search 1000+ websites in a command-line way, with curated and personal shortcuts, organized by namespaces, allowing multiple and typed arguments, with maximum privacy.",
  "main": "",
  "funding": [
    {
      "type": "individual",
      "url": "https://trovu.net/docs/#sponsor"
    }
  ],
  "scripts": {
    "build": "rollup --config --bundleConfigAsCjs --environment INCLUDE_DEPS,BUILD:production",
    "build-blog": "npx @11ty/eleventy",
    "build-docs": "pipenv run mkdocs build",
    "build-jsdoc": "jsdoc --configure jsdoc.json",
    "clean": "rm -rf dist/",
    "compile-data": "node dist/cli.mjs compile-data --output ./dist/public/data.json",
    "dev-server": "http-server ./dist/public/ --port 8081 --cors",
    "lint-js": "npx eslint src/",
    "lint-js-fix": "npx eslint src/ --fix",
    "lint-yaml": "npx yaml-lint 'data/**/*.yml'",
    "normalize-data": "node dist/cli.mjs normalize-data",
    "prettier-fix": "npx prettier -w 'src/**/*.js'",
    "prettier-list": "npx prettier -l 'src/**/*.js'",
    "rollout": "npm run clean && npm run build && npm run test",
    "test": "npm run lint-yaml && npm run test-unit && npm run test-calls && npm run test-fe",
    "test-calls": "jest --verbose calls",
    "test-fe": "cypress run",
    "test-shortcuts": "node dist/cli.mjs test-shortcuts",
    "test-unit": "jest --verbose env helper src/ts/ ",
    "validate-data": "node dist/cli.mjs validate-data",
    "watch": "rollup --config --watch --bundleConfigAsCjs",
    "we-update-assets": "cp node_modules/countries-list/dist/minimal/countries.en.min.json node_modules/countries-list/dist/minimal/languages.en.min.json src/web-ext/json",
    "we-upload-firefox": "web-ext sign --source-dir src/web-ext --channel=listed --amo-metadata=src/web-ext/metadata.json --api-key=`pass show addons.mozilla.org | grep '^jwt-issuer: ' | cut -d ' ' -f 2` --api-secret=`pass show addons.mozilla.org | grep '^jwt-secret: ' | cut -d ' ' -f 2`",
    "we-run-firefox": "web-ext run --source-dir src/web-ext/",
    "we-zip-chrome": "cd src/web-ext && zip -r web-ext.zip . && mv web-ext.zip ../../dist/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trovu/trovu.git"
  },
  "keywords": [
    "javascript",
    "shortcuts",
    "social",
    "bootstrap"
  ],
  "author": "Georg Jaehnig",
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/trovu/trovu/issues"
  },
  "homepage": "https://trovu.net/",
  "jest": {},
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-img": "^3.1.8",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@11ty/eleventy-plugin-bundle": "^1.0.4",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.12.1",
    "@cospired/i18n-iso-languages": "github:georgjaehnig/i18n-iso-languages#new-languages",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.14.0",
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-html": "^1.0.4",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-replace": "^6.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "^22.8.1",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "ajv": "^8.12.0",
    "commander": "^11.1.0",
    "cypress": "^13.6.1",
    "eslint-plugin-jest": "^28.9.0",
    "eslint-plugin-mozilla": "^4.0.0",
    "esm": "^3.2.25",
    "globals": "^15.12.0",
    "http-server": "^14.1.1",
    "is-valid-domain": "^0.1.6",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^27.3.1",
    "jsdoc": "^4.0.2",
    "languagelist": "github:georgjaehnig/language-list",
    "luxon": "^3.3.0",
    "markdown-it-anchor": "^8.6.7",
    "prettier": "^3.1.0",
    "rollup": "^4.24.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-css-only": "^4.5.2",
    "rollup-plugin-execute": "^1.1.1",
    "rollup-plugin-scss": "^4.0.0",
    "rollup-plugin-watch": "^1.0.4",
    "sass": "^1.39.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "yaml-lint": "^1.7.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.6.0",
    "bootstrap": "^5.3.3",
    "bootstrap.native": "^5.0.13",
    "countries-list": "^2.6.1",
    "dayjs": "^1.11.7",
    "detect-browser": "^5.2.0",
    "escape-string-regexp": "^4.0.0",
    "js-yaml": "^4.1.0",
    "node-fetch": "^3.3.2",
    "split-limit": "^1.0.5"
  }
}
